
# Пример файла конфигурации для dela - системы управления сотрудниками
# Скопируйте этот файл в .env и заполните своими значениями

# Настройки базы данных PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Настройки сессий (минимум 32 символа)
SESSION_SECRET=your_very_secure_session_secret_key_min_32_chars_here

# Настройки сервера
PORT=5000
NODE_ENV=development

# Дополнительные настройки (опционально)
# CORS_ORIGIN=http://localhost:3000
# LOG_LEVEL=info
